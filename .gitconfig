[user]
  name = kano4
  email = shinji.warp@gmail.com
[color]
  ui = auto
[core]
  editor = vim
[alias]
  st = status
  ci = commit
  co = checkout
  di = diff
  br = branch
  unstage = reset HEAD --
  last = log -1 HEAD
  lo = log
  ad = add
  pl = pull
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
[github]
  user = kano4
